FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.2";
    object      deltaT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];


internalField   uniform 0;

boundaryField
{
    hotSide
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
    coldSide
    {
        type            mixed;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 0;
        value           uniform 0;
    }
    adiabaticPlanes
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
}


