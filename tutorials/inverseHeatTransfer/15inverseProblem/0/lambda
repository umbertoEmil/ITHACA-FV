FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      lambda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];


internalField   uniform 0;

boundaryField
{
    hotSide
    {
        type            fixedGradient;
        gradient        uniform 0.0;
    }
    coldSide
    {
        type            mixed;
        refValue        uniform 300;
        refGradient     uniform 0;
        valueFraction   uniform 1.7133992e-4;
    }
    adiabaticPlanes
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
}
